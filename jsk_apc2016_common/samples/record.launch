<launch>
  <arg name="bagfile" value="fetch_plane_detection.bag"/>
  <node name="rosbag_record"
        pkg="rosbag" type="record"
        args="-O $(arg bagfile)
              -b 0

	      /head_camera/rgb/image_rect_color
	      /tf
	      /joint_states
	      /base_link
	      /move_group/monitored_planning_scene
	      /collision_object
	      "
        output="screen">
  </node>
</launch>

